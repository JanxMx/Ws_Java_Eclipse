Connection to Internet is required!!!

--------------- Hibernate CRUD -----------------

/* create database jwt; */
use janx;

CREATE TABLE `empcrud` (
  `ID` int(11) NOT NULL,
  `NAME` varchar(255) NOT NULL,
  `SAL` int(11) NOT NULL,
  `CITY` varchar(255) NOT NULL,
  `PHONE` int(11) NOT NULL,
  PRIMARY KEY (`ID`)
) ENGINE=InnoDB DEFAULT CHARSET=latin1;

